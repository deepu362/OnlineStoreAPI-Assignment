	"item": [
		{
			"name": "login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const jason_response = pm.response.json();\r",
							"pm.test(\"Validate successful response\", function(){\r",
							"    // validating successful response of request with 200 code\r",
							"    pm.response.to.have.status(200)\r",
							"    // validating successful response of request with status code exactly equal to 200\r",
							"    pm.expect(pm.response.code).to.be.eql(200)\r",
							"    // validating successful response of request to have OK\r",
							"    pm.expect(pm.response.status).to.be.eql(\"OK\")\r",
							"});\r",
							"\r",
							"pm.test(\"Validating response\", function(){\r",
							"    // Validating if response have token property\r",
							"    pm.expect(jason_response).to.have.property(\"token\")\r",
							"    // Validating the reponse token is not empty\r",
							"    pm.expect(jason_response.token).to.be.a('string').and.not.be.empty\r",
							"});\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Assigning the global variables[username, password] data from the csv file\r",
							"pm.globals.set(\"username\", pm.iterationData.get(\"username\"));\r",
							"pm.globals.set(\"password\", pm.iterationData.get(\"password\"));"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"username\": \"{{username}}\",\r\n    \"password\": \"{{password}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{base_url}}auth/login",
					"host": [
						"{{base_url}}auth"
					],
					"path": [
						"login"
					]
				},
				"description": "In this POST request we are logging in the user and getting a token in response so we are validating:\n\n- successful response\n    \n- token is returned and having a string value"
			},
			"response": []
		},
